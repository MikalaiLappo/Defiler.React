{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "outDir": "./dist"
  },
  "exclude": ["./node_modules/**/*", "./dist"]
}
